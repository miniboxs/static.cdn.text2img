#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/aircode/node_modules/.pnpm/@aircodelabs+hydrogen@0.7.0/node_modules/@aircodelabs/hydrogen/bin/node_modules:/home/aircode/node_modules/.pnpm/@aircodelabs+hydrogen@0.7.0/node_modules/@aircodelabs/hydrogen/node_modules:/home/aircode/node_modules/.pnpm/@aircodelabs+hydrogen@0.7.0/node_modules/@aircodelabs/node_modules:/home/aircode/node_modules/.pnpm/@aircodelabs+hydrogen@0.7.0/node_modules:/home/aircode/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/aircode/node_modules/.pnpm/@aircodelabs+hydrogen@0.7.0/node_modules/@aircodelabs/hydrogen/bin/node_modules:/home/aircode/node_modules/.pnpm/@aircodelabs+hydrogen@0.7.0/node_modules/@aircodelabs/hydrogen/node_modules:/home/aircode/node_modules/.pnpm/@aircodelabs+hydrogen@0.7.0/node_modules/@aircodelabs/node_modules:/home/aircode/node_modules/.pnpm/@aircodelabs+hydrogen@0.7.0/node_modules:/home/aircode/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/deploy.js" "$@"
else
  exec node  "$basedir/../../bin/deploy.js" "$@"
fi
